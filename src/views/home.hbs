{{!toJSON data}}
{{#section 'head'}}
<style>

</style>
{{/section}}

<h4>{{data.reportName}}</h4>

<table class="table table-bordered table-sm">
    <thead class="text-center">
        {{#each data.reportHeader.rows}}
        <tr>
            {{#each columns}}
            <th rowspan="{{rowspan}}" colspan="{{colspan}}">{{value}}</th>
            {{/each}}
        </tr>
        {{/each}}
    </thead>

    <tbody>
        {{#each data.reportBody.rows}}
        <tr>
            {{#each columns}}
            <td>
                {{value}}
            </td>
            {{/each}}
        </tr>
        {{/each}}
    </tbody>
</table>