{{!toJSON data}}
{{#section 'head'}}
<style>

</style>
{{/section}}

<h4>{{data.reportName}}</h4>

<table class="table table-bordered table-sm">
    <thead class="text-center">
        {{#each data.header.headerRows}}
        <tr>
            {{#each this}}
            <th rowspan="{{rowspan}}" colspan="{{colspan}}">{{value}}</th>
            {{/each}}
        </tr>
        {{/each}}

    </thead>


    <tbody>
        {{#each data.rows}}
        <tr>
            <td>
                {{kpiNameCell.name}}
            </td>
            {{#each dimensions}}
            <td>
                {{rowDim}}
            </td>
            {{/each}}
            {{#each valuesv1}}
            <td>
                {{measure}}
            </td>
            {{/each}}
        </tr>
        {{/each}}
    </tbody>
</table>